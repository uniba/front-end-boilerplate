{
  "name": "front-end-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "start": "node index.js",
    "supervisor": "supervisor --exec ./node_modules/.bin/babel-node index.js",
    "test": "ava --source test",
    "lint:scripts": "esw",
    "serve": "node index.js",
    "build": "bash bin/build",
    "clean": "bash bin/clean",
    "watch": "bash bin/watch",
    "build:scripts": "NODE_ENV=build browserify src/javascripts/app.js -p licensify | uglifyjs --comments '/generated by licensify/' > build/javascripts/app.js",
    "build:styles": "NODE_ENV=build postcss --config .postcss.js src/stylesheets/app.css | cssnano > build/stylesheets/app.css",
    "watch:scripts": "watchify src/javascripts/app.js --verbose --debug -o ./.tmp/javascripts/app.js",
    "watch:styles": "postcss --config .postcss.js src/stylesheets/app.css --output ./.tmp/stylesheets/app.css --watch --map"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "cssnano-cli": "^1.0.4",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "licensify": "^3.1.2",
    "morgan": "^1.7.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^2.5.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-devtools": "^1.1.0",
    "postcss-import": "^8.1.2",
    "postcss-reporter": "^1.4.1",
    "precss": "^1.4.0",
    "rucksack-css": "^0.8.6",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "sugarss": "^0.1.5",
    "supervisor": "^0.11.0",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  }
}
