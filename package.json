{
  "name": "front-end-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "start": "node index.js",
    "supervisor": "supervisor --exec ./node_modules/.bin/babel-node index.js",
    "test": "ava --source test",
    "lint:scripts": "esw",
    "format:scripts": "eslint . --fix",
    "format:styles": "stylefmt --recursive 'src/**/*.css'",
    "serve": "node index.js",
    "build": "bash bin/build",
    "clean": "bash bin/clean",
    "watch": "bash bin/watch",
    "build:scripts": "NODE_ENV=production webpack --progress --bail -p",
    "build:styles": "NODE_ENV=build postcss --config .postcss.js src/stylesheets/app.css | csso > build/stylesheets/app.css",
    "watch:scripts": "NODE_ENV=development webpack --progress --watch -d",
    "watch:styles": "postcss --config .postcss.js src/stylesheets/app.css --output ./.tmp/stylesheets/app.css --watch --map"
  },
  "devDependencies": {
    "ava": "0.18.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.23.0",
    "css-loader": "0.26.1",
    "csso": "2.3.1",
    "eslint": "3.15.0",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.0.1",
    "eslint-watch": "3.0.0",
    "express": "4.14.1",
    "morgan": "1.8.1",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cli": "2.6.0",
    "postcss-cssnext": "2.9.0",
    "postcss-devtools": "1.1.1",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.0",
    "postcss-reporter": "3.0.0",
    "style-loader": "0.13.1",
    "stylefmt": "5.1.2",
    "stylelint": "7.8.0",
    "stylelint-config-standard": "16.0.0",
    "supervisor": "0.12.0",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "normalize.css": "5.0.0"
  }
}
