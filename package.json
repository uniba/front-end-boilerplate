{
  "name": "front-end-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "start": "node index.js",
    "supervisor": "supervisor --exec ./node_modules/.bin/babel-node index.js",
    "test": "ava --source test",
    "lint:scripts": "esw",
    "serve": "node index.js",
    "build": "bin/build",
    "clean": "bin/clean",
    "watch": "bin/watch",
    "build:scripts": "browserify src/javascripts/app.js -p licensify | uglifyjs --comments '/generated by licensify/' > build/javascripts/app.js",
    "build:styles": "postcss --config .postcss.js src/stylesheets/app.css | cssnano > build/stylesheets/app.css",
    "watch:scripts": "watchify src/javascripts/app.js --outfile='exorcist ./.tmp/javascripts/app.js.map > ./.tmp/javascripts/app.js' --verbose -d",
    "watch:styles": "postcss --config .postcss.js src/stylesheets/app.css --output ./.tmp/stylesheets/app.css --watch --map"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "babelify": "^7.2.0",
    "cssnano-cli": "^1.0.3",
    "eslint": "^2.6.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "eslint-watch": "^2.1.10",
    "exorcist": "^0.4.0",
    "express": "^4.13.4",
    "licensify": "^3.1.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cli": "^2.5.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-devtools": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "precss": "^1.4.0",
    "rucksack-css": "^0.8.5",
    "sugarss": "^0.1.2",
    "supervisor": "^0.10.0",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  }
}
