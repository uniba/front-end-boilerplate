{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "Explorer >= 11",
          "Android >= 4",
          "iOS >= 9"
        ]
      },
      "modules": false,
      "useBuiltIns": true
    }],
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": true,
      "polyfills": false,
      "regenerator": true,
    }]
  ]
}
